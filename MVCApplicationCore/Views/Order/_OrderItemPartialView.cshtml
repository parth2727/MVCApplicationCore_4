@model List<OrderItem>
<table class="table">
    <thead>
        <tr>
            <th>Order Item Id</th>
            <th>Order Id</th>
            <th>Item Name</th>
            <th>Item Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderItemId</td>
                <td>@order.OrderId</td>
                <td>@order.ItemName</td>
                <td>@order.ItemPrice</td>
            </tr>
            
        }
    </tbody>
</table>